{
    "name": "element-plus",
    "displayName": "element-plus",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.89.1"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.helloWorld",
        "onLanguage:html",
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "snippets": [
            {
                "language": "vue",
                "path": "./snippets/template.code-snippets"
            },
            {
                "language": "html",
                "path": "./snippets/template.code-snippets"
            }
        ],
        "languages": [
            {
                "id": "html",
                "extensions": [
                    ".html",
                    ".vue"
                ],
                "aliases": [
                    "HTML",
                    "html"
                ]
            }
        ],
        "completionItemProvider": [
            {
                "language": "html",
                "triggerCharacters": [
                    "<",
                    " "
                ]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.9",
        "@types/node": "20.x",
        "@types/vscode": "^1.95.0",
        "@typescript-eslint/eslint-plugin": "^8.10.0",
        "@typescript-eslint/parser": "^8.7.0",
        "@vscode/test-cli": "^0.0.10",
        "@vscode/test-electron": "^2.4.1",
        "eslint": "^9.13.0",
        "typescript": "^5.6.3"
    },
    "dependencies": {
        "vscode-html-languageservice": "^5.3.1"
    }
}
